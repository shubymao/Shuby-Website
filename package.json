{
  "name": "shuby-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lintStyle": "stylelint **/*.css --fix "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
