    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "prettier": "2.5.1",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "tailwindcss": "3.0.7",
    "typescript": "4.5.4",
    "url-loader": "4.1.1"
  }
