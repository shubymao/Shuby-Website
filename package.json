    "postcss-import": "14.0.2",
    "prettier": "2.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss": "2.1.4",
    "typescript": "4.3.2",
    "url-loader": "4.1.1"
  }
