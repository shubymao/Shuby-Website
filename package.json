    "@fortawesome/react-fontawesome": "0.1.16",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react-responsive": "8.0.5",
    "emailjs-com": "3.2.0",
    "gray-matter": "4.0.3",
    "gsap": "3.9.0",
    "next": "12.0.7",
    "next-transpile-modules": "9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "7.1.1",
    "react-responsive": "8.2.0",
    "react-syntax-highlighter": "15.4.5",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
